{
  "env_name":"Hopper-v2",
  "unity_env": "False",
  "state_size": [4,11],
  "action_size": 3,
  "seed": 700,

  "agent": {
            "network": "MLP",
            "actor": {
              "num_of_layers": 2,
              "filter_size": [256,256],
              "activation": "relu",
              "actor_activation": "linear",
              "batch_norm": "None"
            },
            "critic": {
              "num_of_layers": 3,
              "activation": "relu",
              "batch_norm": "None",
              "filter_size": [256,256,1]
            }
  },

  "tau": 0.005,
  "conservative_mode": "True",
  "run_step": 300000,
  "start_step": 10000,
  "show_episode": 5,


  "fixed_temperature": "None",

  "size_replay_memory": 1e6,
  "batch_size": 256,
  "learning_freq": 1,
  "reward_scaling": 1,
  "gradient_steps": 1,

  "optimizer": {
    "actor": {
      "name": "adam",
      "learning_rate": 3e-4,
      "weight_decay": 0,
      "eps": 1e-7,
      "clipping": "None"
    },
    "critic": {
      "name": "adam",
      "learning_rate": 3e-4,
      "weight_decay": 0,
      "clipping": "None",
      "eps": 1e-7
    },
    "temperature": {
      "name": "adam",
      "learning_rate": 3e-4,
      "weight_decay": 0,
      "clipping": "None",
      "eps": 1e-7
    }
  },

  "discount_factor": 0.99,

  "save_path": "./save/04_SAC/",
  "tensorboard_path": "./tensorboard/04_SAC/",
  "load_path":"None",

  "is_gpu": "True",
  "gpu_name": "cuda:0",
  "inference_mode": "False",
  "render_mode": "False"
}
