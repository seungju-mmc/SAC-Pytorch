  "optimizer": {
    "name":"adam",
    "learning_rate": 0.001,
    "weight_decay": 5e-4,
    "momentum": "None",
    "eps": 0.01,
    "alpha": 0.95,
    "clipping": "None"
  },
  "discount_factor": 0.99,

  "save_path": "./save/01_DQN/",
  "tensorboard_path": "./tensorboard/01_DQN/",
  "load_path":"None",

  "is_gpu": "True",
  "gpu_name": "cuda:0",
  "Exploration_method": "e_greedy",
  "epsilon_min": 0.1,
  "epsilon": 1.0,
  "inference_mode": "False",
  "render_mode": "False"
}
