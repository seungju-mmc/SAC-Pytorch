{
  "env_name":"CartPole-v0",
  "unity_env": "False",
  "state_size": [2,4],
  "action_size": 2,

  "agent": {
            "dueling": "True",
            "network": "MLP",

            "num_of_layers": 1,
            "filter_size": [200],
            "activation": "relu",
            "fully_connected_size":200
  },


  "run_step": 10000,
  "start_step": 1000,
  "update_step": 1000,
  "show_episode": 50,

  "size_replay_memory": 3000,
  "batch_size": 32,
